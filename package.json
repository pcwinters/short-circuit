{
  "name": "short-circuit",
  "version": "0.0.1-2",
  "description": "Short Circuit Flux/React Data Management",
  "main": "lib/index.js",
  "browser": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "./node_modules/.bin/rimraf lib/*",
    "lint": "./node_modules/.bin/eslint src test",
    "build:mkdir": "mkdir -p lib",
    "watch": "npm run build:mkdir && ./node_modules/.bin/watchify -x react -e src/index.js -o lib/index.js",
    "build": "npm run build:mkdir && ./node_modules/.bin/browserify -x react -e src/index.js -o lib/index.js",
    "test": "./node_modules/.bin/karma start --single-run",
    "preversion": "npm run clean && npm run lint",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pcwinters/short-circuit.git"
  },
  "keywords": [
    "react",
    "flux",
    "relay",
    "redux"
  ],
  "author": "Patrick Winters <patrick.winters@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pcwinters/short-circuit/issues"
  },
  "homepage": "https://github.com/pcwinters/short-circuit#readme",
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.2.0",
    "coffee-script": "^1.10.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "karma": "^0.13.19",
    "karma-browserify": "^5.0.1",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "mocha": "^2.3.4",
    "q": "^1.4.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-stub-context": "^0.4.0",
    "reselect": "^2.0.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "skin-deep": "^0.14.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "baconipsum": "^0.1.2",
    "invariant": "^2.2.0",
    "warning": "^2.1.0"
  }
}
